{
  "name": "puresend",
  "version": "0.1.0",
  "description": "跨平台文件传输应用，基于 Tauri 2 + Vue 3 + TypeScript 构建",
  "author": "PureSend Contributors",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/z-only/puresend.git"
  },
  "bugs": {
    "url": "https://github.com/z-only/puresend/issues"
  },
  "homepage": "https://z-only.github.io/puresend/",
  "keywords": [
    "tauri",
    "vue3",
    "file-transfer",
    "cross-platform",
    "p2p",
    "desktop-app"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint",
    "format": "prettier . --write --ignore-unknown",
    "format:check": "prettier . --check --ignore-unknown",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-clipboard-manager": "~2",
    "@tauri-apps/plugin-dialog": "~2",
    "@tauri-apps/plugin-fs": "^2.4.5",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-shell": "~2.3.5",
    "@tauri-apps/plugin-store": "^2.4.2",
    "pinia": "^3.0.4",
    "qrcode": "^1.5.4",
    "vite-plugin-vuetify": "^2.1.3",
    "vue": "^3.5.28",
    "vue-i18n": "^11.2.8",
    "vue-router": "^5.0.3",
    "vuetify": "^3.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@mdi/js": "^7.4.47",
    "@tauri-apps/cli": "^2",
    "@types/node": "^25.3.0",
    "@types/qrcode": "^1.5.6",
    "@vitejs/plugin-vue": "^6.0.4",
    "canvas": "^3.2.1",
    "eslint": "^10.0.1",
    "eslint-plugin-vue": "^10.8.0",
    "globals": "^17.3.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "stylelint": "^17.3.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.0",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^7.3.1",
    "vite-plugin-eslint": "^1.8.1",
    "vitepress": "^1.6.4",
    "vue-tsc": "^3.2.5"
  }
}